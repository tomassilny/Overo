<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tosiapps.materialbarcodescannerexample">

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.tosiapps.materialbarcodescannerexample.MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.tosiapps.materialbarcodescannerexample.MenuActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tosiapps.materialbarcodescannerexample.WebSite"
            android:screenOrientation="portrait"/>
        <activity android:name="com.tosiapps.materialbarcodescannerexample.Verify"
            android:screenOrientation="portrait"></activity>
        <activity android:name="com.tosiapps.materialbarcodescannerexample.InfoActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>